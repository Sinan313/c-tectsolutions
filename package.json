{
  "name": "c-tech",
  "version": "1.0.0",
  "description": "C-Tech Solutions Education Platform",
  "main": "server/server.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon server/server.js",
    "client:dev": "http-server -c-1 C\\ tech\\ Solutions -p 3000",
    "build": "npm run build:client && npm run build:server",
    "build:client": "mkdir -p dist && cp -r C\\ tech\\ Solutions/* dist/ && cp -r Portfolio dist/ && cp *.html *.css *.js dist/",
    "build:server": "echo 'Server build completed'",
    "start": "node server/server.js",
    "db:setup": "node server/setup-db.js",
    "db:seed": "node server/seed-db.js",
    "test": "jest",
    "lint": "eslint server/**/*.js *.js",
    "deploy": "npm run build && npm run deploy:server",
    "deploy:server": "echo 'Deploy to your hosting platform'"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "multer": "^2.0.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "eslint": "^9.36.0",
    "grunt": "^1.6.1",
    "grunt-cli": "^1.5.0",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "education",
    "learning",
    "platform",
    "c-tech"
  ],
  "author": "C-Tech Solutions",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sinan313/c-tectsolutions.git"
  },
  "bugs": {
    "url": "https://github.com/Sinan313/c-tectsolutions/issues"
  },
  "homepage": "https://github.com/Sinan313/c-tectsolutions#readme"
}
